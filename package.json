{
  "name": "react-picture",
  "version": "0.0.3",
  "description": "Responsive images component for React.JS",
  "main": "./src/index",
  "scripts": {
    "test": "lab -c -L --lint-options '{\"extensions\":[\".js\",\".jsx\"]}' -S -T node_modules/lab-babel",
    "test-cov-html": "lab -c -S -T node_modules/lab-babel -r html -o coverage.html",
    "start": "webpack-dev-server --inline --content-base examples/",
    "compile": "babel -d src/ lib/",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesplat/react-picture.git"
  },
  "keywords": [
    "image",
    "picture",
    "responsive",
    "srcset",
    "react",
    "react-component"
  ],
  "author": "Lois Desplat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ldesplat/react-picture/issues"
  },
  "homepage": "https://github.com/ldesplat/react-picture",
  "devDependencies": {
    "babel": "5.x.x",
    "babel-core": "5.x.x",
    "babel-loader": "5.x.x",
    "code": "1.x.x",
    "lab": "5.x.x",
    "lab-babel": "1.x.x",
    "react": "0.13.x",
    "webpack": "1.x.x",
    "webpack-dev-server": "1.x.x"
  },
  "browserify-shim": {
    "react": "global:React"
  }
}
